{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "types": [
      "@nuxt/types",
      "@types/node",
      "@nuxtjs/axios",
      "vitest/globals"
    ]
  },
  "exclude": ["node_modules"],
  "extends": "./.nuxt/tsconfig.json"
}
